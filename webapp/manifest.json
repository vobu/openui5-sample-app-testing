{
  "sap.app": {
	"id": "sap.ui.demo.todo",
	"type": "application",
	"dataSources": {
	  "mainService": {
		"uri": "/my/odata/SRV",
		"type": "OData",
		"settings": {
		  "odataVersion": "2.0"
		}
	  }
	}
  },
  "sap.ui5": {
	"dependencies": {
	  "libs": {
		"sap.ui.core": {},
		"sap.m": {}
	  }
	},
	"rootView": {
	  "viewName": "sap.ui.demo.todo.view.App",
	  "type": "XML",
	  "async": true,
	  "id": "app"
	},
	"models": {
	  "i18n": {
		"type": "sap.ui.model.resource.ResourceModel",
		"settings": {
		  "bundleName": "sap.ui.demo.todo.i18n.i18n"
		}
	  },
	  "appView": {
		"type": "sap.ui.model.json.JSONModel",
		"uri": "model/appView.json"
	  },
	  "": {
		"dataSource": "mainService",
		"settings": {
		  "defaultBindingMode": "TwoWay"
		}
	  }
	},
	"resources": {
	  "css": [
		{
		  "uri": "css/styles.css"
		}
	  ]
	}
  }
}
